{
  "type": "module",
  "scripts": {
    "dev": "(cd .astro && astro dev)",
    "start": "(cd .astro && astro dev)",
    "build": "(cd .astro && astro build)",
    "preview": "(cd .astro && astro preview)"
  },
  "dependencies": {
    "@astrojs/tailwind": "^3.1.2",
    "@tailwindcss/typography": "^0.5.9",
    "@types/katex": "^0.16.0",
